<odoo>
    <data noupdate="1">
        <!-- Sequence for Truck Vehicle -->
        <record id="seq_truck_vehicle" model="ir.sequence">
            <field name="name">Truck Vehicle Sequence</field>
            <field name="code">truck.vehicle.sequence</field>
            <field name="prefix">TRK</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sequence for Truck Maintenance Request -->
        <record id="seq_truck_maintenance_request" model="ir.sequence">
            <field name="name">Truck Maintenance Request Sequence</field>
            <field name="code">truck.maintenance.request</field>
            <field name="prefix">MNT</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>